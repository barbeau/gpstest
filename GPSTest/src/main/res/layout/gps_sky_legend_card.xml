                    android:layout_width="@dimen/sky_legend_shape_small_line"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    android:background="@color/body_text_2_light" />

                <ImageView
                    android:id="@+id/legend_flag_southpan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    android:background="@color/body_text_2_light"
                    android:padding="1dp"
                    app:srcCompat="@drawable/ic_flag_southpan"
                    android:contentDescription="@string/southpan_flag"/>

                <View
                    android:id="@+id/sky_legend_shape_line17b"
                    android:layout_width="@dimen/sky_legend_shape_small_line"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    android:background="@color/body_text_2_light" />

                <TextView
                    android:id="@+id/sky_southpan_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:text="@string/sky_legend_shape_southpan" />
            </TableRow>
            
        </TableLayout>

        <TextView
            android:id="@+id/sky_shape_in_view_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sky_shape_legend_sbas"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:textStyle="bold"
            android:text="@string/sky_legend_shape_availability_title" />

        <TableLayout
            android:id="@+id/sky_shape_legend_in_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sky_shape_in_view_title"
            android:layout_margin="5dp"
            android:orientation="vertical"
            android:layout_centerHorizontal="true">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/sky_legend_not_in_view"
                    android:layout_width="@dimen/sky_legend_shape_size"
                    android:layout_height="@dimen/sky_legend_shape_size"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    app:srcCompat="@drawable/circle_not_in_view"
                    android:contentDescription="@string/empty_shape"/>

                <View
                    android:id="@+id/sky_legend_shape_line11a"
                    android:layout_width="@dimen/sky_legend_shape_small_line"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    android:background="@color/body_text_2_light" />

                <TextView
                    android:id="@+id/sky_not_in_view_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:text="@string/sky_legend_shape_not_in_view" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/sky_legend_in_view"
                    android:layout_width="@dimen/sky_legend_shape_size"
                    android:layout_height="@dimen/sky_legend_shape_size"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    app:srcCompat="@drawable/circle_in_view"
                    android:contentDescription="@string/filled_shape_no_border"/>

                <View
                    android:id="@+id/sky_legend_shape_line12a"
                    android:layout_width="@dimen/sky_legend_shape_small_line"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    android:background="@color/body_text_2_light" />

                <TextView
                    android:id="@+id/sky_in_view_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:text="@string/sky_legend_shape_in_view" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/sky_legend_used_in_fix"
                    android:layout_width="@dimen/sky_legend_shape_size"
                    android:layout_height="@dimen/sky_legend_shape_size"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    app:srcCompat="@drawable/circle_used_in_fix"
                    android:contentDescription="@string/filled_shape_with_border"/>

                <View
                    android:id="@+id/sky_legend_shape_line13a"
                    android:layout_width="@dimen/sky_legend_shape_small_line"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"
                    android:background="@color/body_text_2_light" />

                <TextView
                    android:id="@+id/sky_used_in_fix_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:text="@string/sky_legend_shape_used_in_fix" />
            </TableRow>
        </TableLayout>
    </RelativeLayout>
</androidx.cardview.widget.CardView>
